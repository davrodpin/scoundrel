{
  "name": "scoundrel-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:frontend": "npm run dev -w @scoundrel/frontend",
    "build:frontend": "npm run build -w @scoundrel/frontend",
    "lint:frontend": "npm run lint -w @scoundrel/frontend",
    "preview:frontend": "npm run preview -w @scoundrel/frontend",
    "deploy:frontend": "npm run deploy -w @scoundrel/frontend",
    "dev:backend": "npm run dev -w @scoundrel/backend",
    "build:backend": "npm run build -w @scoundrel/backend",
    "start:backend": "npm run start -w @scoundrel/backend",
    "lint:backend": "npm run lint -w @scoundrel/backend"
  },
  "devDependencies": {
    "typescript": "~5.3.3"
  }
}
